{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "strict": true,
    "jsx": "react",
    "moduleResolution": "Bundler",
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "types": [
      "react",
      "react-native",
      "expo"
    ]
  },
  "exclude": [
    "node_modules"
  ]
}